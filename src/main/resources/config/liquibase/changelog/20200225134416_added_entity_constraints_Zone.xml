<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Zone.
    -->
    <changeSet id="20200225134416-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="branch_id"
                                 baseTableName="zone"
                                 constraintName="fk_zone_branch_id"
                                 referencedColumnNames="id"
                                 referencedTableName="branch"/>

        <addForeignKeyConstraint baseColumnNames="inventory_id"
                                 baseTableName="zone"
                                 constraintName="fk_zone_inventory_id"
                                 referencedColumnNames="id"
                                 referencedTableName="inventory"/>

    </changeSet>
</databaseChangeLog>
